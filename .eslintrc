{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["prettier", "airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "linebreak-style": ["error", "windows"]
  }
}
